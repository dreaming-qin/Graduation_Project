
{
    // 使用IEMOCAP、训练mmin使用的参数，使用vscode调试时需要放到launch.json中
    "args": [
        //其它
        "--dataset_mode","multimodal_miss",
        "--model","mmin",
        "--corpus","IEMOCAP",
        "--name","mmin_IEMOCAP",
        // --suffix=block_{n_blocks}_run{run_idx}
        "--suffix","block_{n_blocks}_run{run_idx}", 
        "--has_test",
        "--verbose",
        "--in_mem", 
        
        //路径相关
        "--log_dir","./logs",
        "--checkpoints_dir","./checkpoints",
        "--pretrained_path","./checkpoints/CAP_utt_fusion_AVL_run0",

        //一些杂七杂八的参数
        // --cvNo=$i
        "--cvNo","1",
        "--print_freq","10",
