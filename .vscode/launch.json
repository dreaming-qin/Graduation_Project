        "--run_idx","0",
        "--print_freq","10",
        "--gpu_ids","0",
        "--cvNo","1",
        "--embd_size","128", 
        "--feat_compress_size","16,8",
        "--quality","0,95,90,85,80",

        "--A_type","comparE",
        "--input_dim_a","130",
        "--norm_method","trn",
        "--embd_method_a","maxpool",

        "--V_type","efficientface",
