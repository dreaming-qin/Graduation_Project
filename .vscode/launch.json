        // 其它 
        "--has_test",
        "--dataset_mode","multimodal",
        "--model","utt_fusion",
        //--gpu_ids=$gpu 
        "--corpus_name","IEMOCAP",
        // --modality=$modality 
        "--modality","AVL",
        "--save_compress_pic",
        "--type","trad",

        // 路径相关
        "--log_dir","./logs",
        "--checkpoints_dir","./checkpoints",
        "--name","CAP_utt_fusion",
